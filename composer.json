{
    "name": "getloy/getloy-magento-gateway",
    "description": "GetLoy payment gateway for Magento 2.x",
    "require": {
        "php": "~7.1.3||~7.2.0||~7.3.0||~7.4.0",
        "magento/framework": "^102.0",
        "magento/module-checkout": "^100.1",
        "magento/module-config": "^101.0",
        "magento/module-directory": "^100.1",
        "magento/module-payment": "^100.3",
        "magento/module-quote": "^101.1",
        "magento/module-sales": "^102.0",
        "magento/module-store": "^101.0",
        "getloy/getloy-php": "^1.0.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~2.14.0",
        "magento/magento-coding-standard": "~3.0.0",
        "pdepend/pdepend": "2.5.2",
        "phpmd/phpmd": "@stable",
        "sebastian/phpcpd": "~3.0.0",
        "squizlabs/php_codesniffer": "~3.4.0"
    },
    "type": "magento2-module",
    "version": "1.0.2",
    "license": [
        "GPL-3.0-only"
    ],
    "autoload": {
        "files": [ "registration.php" ],
        "psr-4": {
            "Getloy\\GetloyMagentoGateway\\": ""
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ]
}
